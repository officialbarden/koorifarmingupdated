scoreboard players reset * time.ScoreboardDisplay


scoreboard players display name .AA time.ScoreboardDisplay {"text":" "}
$scoreboard players display name .ATime time.ScoreboardDisplay {"text":"Time: $(Hour) $(wtsomd), $(Day)"}
$scoreboard players display name .BDate time.ScoreboardDisplay {"text":"Date: $(DayNum)$(NumberThang), $(Season), Year $(Year)"}
$scoreboard players display name .XHumidity time.ScoreboardDisplay [{"text":"Humidity: $(Humidity)%"}]
scoreboard players display name .Y time.ScoreboardDisplay {"text":" "}
scoreboard players display name .Z time.ScoreboardDisplay ["Remember to ",{"color":"#D1EDE7","text":"/vote"},"!"]
